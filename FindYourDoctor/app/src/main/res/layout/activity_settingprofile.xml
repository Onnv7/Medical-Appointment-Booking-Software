<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background">

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="110dp"
        android:layout_height="30dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="36dp"
        android:orientation="horizontal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="@drawable/backgroud_back"
            android:padding="7dp">

            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:srcCompat="@drawable/back" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:id="@+id/textView27"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center|end"
            android:text="Others"
            android:textColor="#000000"
            android:textSize="18sp"
            android:textStyle="bold" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="500dp"
        android:layout_marginTop="110dp"
        android:orientation="vertical"
        android:paddingStart="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginBottom="20dp"
            android:orientation="horizontal">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="49dp"
                android:layout_height="49dp"
                android:background="@drawable/background_green"
                android:padding="13dp">

                <ImageView
                    android:id="@+id/user_icon"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    app:srcCompat="@drawable/user" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <TextView
                android:id="@+id/user_des"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginStart="20dp"
                android:layout_weight="1"
                android:gravity="center|start"
                android:text="Medical examination history"
                android:textSize="17sp" />

            <ImageView
                android:id="@+id/user_arrow"
                android:layout_width="30dp"
                android:layout_height="match_parent"
                android:layout_margin="12dp"
                app:srcCompat="@drawable/arrow" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginBottom="20dp"
            android:orientation="horizontal">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="49dp"
                android:layout_height="49dp"
                android:background="@drawable/background_green"
                android:padding="13dp">

                <ImageView
                    android:id="@+id/notification_icon"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    app:srcCompat="@drawable/notification" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <TextView
                android:id="@+id/notification_des"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginStart="20dp"
                android:layout_weight="1"
                android:gravity="center|start"
                android:text="Notification"
                android:textSize="17sp" />

            <Switch
                android:id="@+id/switch1"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:layout_marginEnd="10dp"
                android:text="On" />


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginBottom="20dp"
            android:orientation="horizontal">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="49dp"
                android:layout_height="49dp"
                android:background="@drawable/background_green"
                android:padding="13dp">

                <ImageView
                    android:id="@+id/language_icon"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    app:srcCompat="@drawable/language" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <TextView
                android:id="@+id/language_des"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginStart="20dp"
                android:layout_weight="1"
                android:gravity="center|start"
                android:text="Language"
                android:textSize="17sp" />

            <TextView
                android:id="@+id/textView31"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="end"
                android:text="english" />

            <ImageView
                android:id="@+id/arrow_language"
                android:layout_width="30dp"
                android:layout_height="match_parent"
                android:layout_margin="12dp"
                app:srcCompat="@drawable/arrow" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginBottom="20dp"
            android:orientation="horizontal">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="49dp"
                android:layout_height="49dp"
                android:background="@drawable/background_green"
                android:padding="13dp">

                <ImageView
                    android:id="@+id/addfr_icon"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    app:srcCompat="@drawable/user1" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <TextView
                android:id="@+id/addfr_des"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginStart="20dp"
                android:layout_weight="1"
                android:gravity="center|start"
                android:text="Invite a freind"
                android:textSize="17sp" />

            <ImageView
                android:id="@+id/arrow_addfr"
                android:layout_width="30dp"
                android:layout_height="match_parent"
                android:layout_margin="12dp"
                app:srcCompat="@drawable/arrow" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginBottom="20dp"
            android:orientation="horizontal">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="49dp"
                android:layout_height="49dp"
                android:background="@drawable/background_green"
                android:padding="13dp">

                <ImageView
                    android:id="@+id/heart_icon"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    app:srcCompat="@drawable/heart"
                    tools:layout_editor_absoluteX="13dp"
                    tools:layout_editor_absoluteY="13dp" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <TextView
                android:id="@+id/heart_des"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginStart="20dp"
                android:layout_weight="1"
                android:gravity="center|start"
                android:text="Favourite doctors"
                android:textSize="17sp" />

            <ImageView
                android:id="@+id/imageView9"
                android:layout_width="30dp"
                android:layout_height="match_parent"
                android:layout_margin="12dp"
                app:srcCompat="@drawable/arrow" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginBottom="20dp"
            android:orientation="horizontal">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="49dp"
                android:layout_height="49dp"
                android:background="@drawable/background_green"
                android:padding="13dp">

                <ImageView
                    android:id="@+id/faq_icon"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    app:srcCompat="@drawable/fontisto_language" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <TextView
                android:id="@+id/textView29"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginStart="20dp"
                android:layout_weight="1"
                android:gravity="center|start"
                android:text="FAQS"
                android:textSize="17sp" />

            <ImageView
                android:id="@+id/arrow_faq"
                android:layout_width="30dp"
                android:layout_height="match_parent"
                android:layout_margin="12dp"
                app:srcCompat="@drawable/arrow" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="49dp"
                android:layout_height="49dp"
                android:background="@drawable/background_green"
                android:padding="13dp">

                <ImageView
                    android:id="@+id/imageView8"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    app:srcCompat="@drawable/question" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <TextView
                android:id="@+id/help_des"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginStart="20dp"
                android:layout_weight="1"
                android:gravity="center|start"
                android:text="Help"
                android:textSize="17sp" />

            <ImageView
                android:id="@+id/arrow_help"
                android:layout_width="30dp"
                android:layout_height="match_parent"
                android:layout_margin="12dp"
                app:srcCompat="@drawable/arrow" />
        </LinearLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>